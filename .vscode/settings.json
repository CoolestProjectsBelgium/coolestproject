{
    "search.exclude": {
        "**/*.code-search": false,
        "**/bower_components": false,
        "**/node_modules": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "yaml"
    ],
    "eslint.alwaysShowStatus": true,
    "githubPullRequests.ignoredPullRequestBranches": [
        "2022"
    ]
}